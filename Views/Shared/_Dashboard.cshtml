@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="pl">

<head>
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" type="text/css" href="~/css/custom.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css">
</head>

<body id="body-pd">
    <script type="text/javascript" src="~/js/site.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js"></script>

    <header class="header" id="header">
        <div class="header_toggle"> <i class='bx bx-menu' id="header-toggle"></i> </div>
    </header>
    <div class="l-navbar" id="nav-bar">
        <nav class="nav">
            <div>
                <span class="nav_logo-name" id="navLogoName">BOK</span><span class="nav_logo_name">- Biuro obsługi klienta</span>
                <div class="nav_list">
                    <a class="nav_link active" asp-area="" asp-controller="Dashboard" asp-action="Dashboard">
                        <i class='bx bx-grid-alt nav_icon'></i>
                        <span class="nav_name">Strona główna</span>
                    </a>
                    
                    <a class="nav_link" asp-area="" asp-controller="Users" asp-action="Index">
                        <i class='bx bx-user nav_icon'></i> <span class="nav_name">Użytkownicy</span>
                    </a>
                    
                    <a class="nav_link" asp-area="" asp-controller="" asp-action="">
                        <i class='bx bx-message-square-detail nav_icon'></i> <span class="nav_name">Powiadomienia</span>
                    </a>
                    <a class="nav_link" asp-area="" asp-controller="" asp-action="">
                        <i class='bx bx-bar-chart-alt-2 nav_icon'></i> <span class="nav_name">Raporty</span>
                    </a>
                    <a class="nav_link" asp-area="" asp-controller="Offers" asp-action="Index">
                        <i class='bx bx-money-withdraw nav_icon'></i> <span class="nav_name">Oferta</span>
                    </a>
                    <a class="nav_link" asp-area="" asp-controller="#" asp-action="#">
                        <i class='bx bxs-calculator nav_icon'></i> <span class="nav_name">Kalkulator</span>
                    </a>
                    <a class="nav_link" asp-area="" asp-controller="#" asp-action="#">
                        <i class='bx bx-calendar nav_icon'></i> <span class="nav_name">Kalendarz</span>
                    </a>
                </div>
            </div>
            <div class="dropdown">
                <a href="#" class="d-flex align-items-center text-white text-decoration-none dropdown-toggle"
                   id="dropdownUser1" data-bs-toggle="dropdown" aria-expanded="false" onclick="sidebar()">
                    <img src="data:image/jpeg;base64," alt="" width="32" height="32" class="rounded-circle me-2"><span class="nav_name">Nazwa użytkownika</span>
                </a>
                <ul class="dropdown-menu dropdown-menu-dark text-small shadow" aria-labelledby="dropdownUser1">
                    <li><a class="dropdown-item" href="#">New project</a></li>
                    <li><a class="dropdown-item" href="#">Settings</a></li>
                    <li><a class="dropdown-item" href="#">Profile</a></li>
                    <li>
                        <hr class="dropdown-divider">
                    </li>
                    <li><a class="dropdown-item" href="#">Sign out</a></li>
                </ul>
            </div>
        </nav>
    </div>
    <div class="height-100">
        @RenderBody();
    </div>
</body>

</html>